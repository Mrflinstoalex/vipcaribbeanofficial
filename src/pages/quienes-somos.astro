---
import Layout from "@/layouts/Layout.astro";
import QuienesSomosPage from "@/components/pages/QuienesSomos";
import { getPageInfo } from '@/lib/wp';
export const prerender = false;

let pageData: any = null;
let error: string | null = null;

try {
  pageData = await getPageInfo('quienes-somos');
} catch (e) {
  console.error('Error cargando página "Quiénes Somos":', e);
  error = 'No se pudo cargar la información de la página en este momento. Por favor, intenta más tarde.';
  // pageData queda como null
}
---

<Layout>
 <QuienesSomosPage 
    data={pageData} 
    error={error} 
    client:load 
  />
</Layout>
